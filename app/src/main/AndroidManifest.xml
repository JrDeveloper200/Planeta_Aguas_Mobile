<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.agua.planeta.planeta_das_aguas_mobile">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_oficial"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_oficial"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.gms.wallet.api.enabled"
            android:value="true" />

        <activity android:name="br.com.agua.planeta.planeta_das_aguas_mobile.Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="br.com.agua.planeta.planeta_das_aguas_mobile.Activity.TelaFornecedores"
            android:parentActivityName="br.com.agua.planeta.planeta_das_aguas_mobile.Activity.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="br.com.agua.planeta.planeta_das_aguas_mobile.Activity.MainActivity" />
        </activity>
        <activity
            android:name="br.com.agua.planeta.planeta_das_aguas_mobile.Activity.TelaQuemSomos"
            android:parentActivityName="br.com.agua.planeta.planeta_das_aguas_mobile.Activity.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="br.com.agua.planeta.planeta_das_aguas_mobile.Activity.MainActivity" />
        </activity>
        <activity
            android:name="br.com.agua.planeta.planeta_das_aguas_mobile.Activity.TelaLocalizacao"
            android:parentActivityName="br.com.agua.planeta.planeta_das_aguas_mobile.Activity.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="br.com.agua.planeta.planeta_das_aguas_mobile.Activity.MainActivity" />
        </activity>
        <activity
            android:name="br.com.agua.planeta.planeta_das_aguas_mobile.Activity.LoginActivity"
            android:parentActivityName="br.com.agua.planeta.planeta_das_aguas_mobile.Activity.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="br.com.agua.planeta.planeta_das_aguas_mobile.Activity.MainActivity" />
        </activity>
        <activity
            android:name="br.com.agua.planeta.planeta_das_aguas_mobile.Activity.TelaPerfil"
            android:parentActivityName="br.com.agua.planeta.planeta_das_aguas_mobile.Activity.LoginActivity">
            <meta-data
                android:name="android.support.PARENT.ACTIVITY"
                android:value=".LoginActivity" />
        </activity>
        <activity
            android:name="br.com.agua.planeta.planeta_das_aguas_mobile.Activity.TelaCadastro"
            android:parentActivityName="br.com.agua.planeta.planeta_das_aguas_mobile.Activity.LoginActivity">
            <meta-data
                android:name="android.support.PARENT.ACTIVITY"
                android:value=".LoginActivity" />
        </activity>
        <activity android:name="br.com.agua.planeta.planeta_das_aguas_mobile.Activity.ProductActivity" />
        <activity android:name="br.com.agua.planeta.planeta_das_aguas_mobile.Activity.CheckoutActivity" />
        <activity android:name="br.com.agua.planeta.planeta_das_aguas_mobile.Activity.PagamentoActivity"></activity>
    </application>

</manifest>